  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Serveny/bullshit_bingo"
  },
  "license": "MIT",
  "nodemonConfig": {
    "watch": [
      "app.js",
      "scripts/"
    ],
    "ext": "ts",
    "ignore": [
      "public/js/*.js"
    ]
  },
  "scripts": {
    "start": "node app.js",
    "start-dev": "@powershell -Command $env:DEBUG='bb'; nodemon app.js",
    "pm2-start": "pm2 start app.js",
    "kill-win": "taskkill /im node.exe",
    "kill-lin": "killall node",
    "restart": "npm run kill-win && npm run start",
    "createDB": "node ./scripts/bb-create-db.js"
  },
  "devDependencies": {
    "@types/jquery": "3.5.14",
    "@types/node": "18.0.0",
    "@types/socket.io": "3.0.1",
    "ts-loader": "9.3.1",
    "typescript": "4.7.4",
    "var_dump": "1.0.5",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0"
  },
  "dependencies": {
    "config": "3.3.7",
    "debug": "4.3.4",
    "express": "4.18.1",
    "pg": "8.7.3",
    "serve-favicon": "2.5.0",
    "shortid": "2.2.16",
    "socket.io": "4.5.1"
  }
